<!DOCTYPE HTML>
<html>

<body>
    
    
    <div class="header-banner">
        <div class="wrap">
            <div class="banner">
                <div class="indent">
                    <h2 class="indent-left margin-bot">Plan the Easy Trip</h2>
                    <div class="banner-left">
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web4/images/glass.png" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Resdaranlar</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web4/images/flight.png" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Ucuslar</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web4/images/group.png" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p> Gedilecek yerler</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web4/images/key.png" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Icareye masinlar</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="header_bottom_right_images">

                        <div class="slider">
                            <ul class="rslides" id="slider1">



                                @foreach (var w in Model)
                                {
                                <li><img src="@w.BLOGIMAGE"style="width:650px;height:400px" alt=""></li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
    </div>
    <div class"content">
        <div class="wrap">
            <div class="wrapper">
                <div class="con-top">
                    <h3>Gezi blogmuza xos geldiniz</h3>
                    
                </div>
                @Html.Action("partial","Default")


                <div class="grids">
                    <div class="box1">
                        @Html.Action("partial3","Default")
                        
                        @Html.Action("partial4","Default")
                        <div class="clear"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
</body>
</html>
